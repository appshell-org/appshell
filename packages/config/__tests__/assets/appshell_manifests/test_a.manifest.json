{
  "remotes": {
    "TestModuleA/Foo": {
      "id": "TestModule/Foo",
      "url": "http://localhost:8080/remoteEntry.js",
      "metadata": {
        "route": "/foo",
        "displayName": "Foo App",
        "displayGroup": "main",
        "order": 10,
        "icon": "ViewList"
      }
    }
  },
  "environment": {
    "test_module_a": {
      "RUNTIME_ENV": "${RUNTIME_ENV}",
      "RUNTIME_ENV_VERSION": "${RUNTIME_ENV_VERSION}"
    }
  },
  "modules": {
    "TestModuleA": {
      "exposes": {
        "./Foo": "./src/Entry1"
      },
      "filename": "remoteEntry.js",
      "library": {
        "name": "some shared lib",
        "type": "umd"
      },
      "name": "TestModule",
      "remotes": {
        "./Remote1": {
          "external": "external module",
          "shareScope": "@shared"
        }
      },
      "remoteType": "amd",
      "runtime": "runtimeChunk",
      "shared": {
        "package1": {
          "singleton": true,
          "requiredVersion": "0.1.0"
        },
        "package2": {
          "singleton": true,
          "requiredVersion": "1.1.0"
        }
      },
      "shareScope": "@sharedscope"
    }
  }
}