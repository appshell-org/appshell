{
  "remotes": {
    "TestModuleBar/Bar": {
      "id": "TestModule/Bar",
      "url": "${APPS_TEST_URL}",
      "scope": "TestModule",
      "module": "./Bar",
      "metadata": {
        "route": "/bar",
        "displayName": "Bar App",
        "displayGroup": "main",
        "order": 20,
        "icon": "ViewList"
      }
    }
  },
  "environment": {
    "test_module_bar": {
      "RUNTIME_ENV": "${RUNTIME_ENV}",
      "RUNTIME_ENV_VERSION": "${RUNTIME_ENV_VERSION}"
    }
  },
  "modules": {
    "TestModuleBar": {
      "exposes": {
        "./Foo": "./src/Entry1",
        "./Bar": "./src/Entry2"
      },
      "filename": "remoteEntry.js",
      "library": {
        "name": "some shared lib",
        "type": "umd"
      },
      "name": "TestModuleBar",
      "remotes": {
        "./Remote1": {
          "external": "external module",
          "shareScope": "@shared"
        }
      },
      "remoteType": "amd",
      "runtime": "runtimeChunk",
      "shared": {
        "package1": {
          "singleton": true,
          "requiredVersion": "0.1.0"
        },
        "package2": {
          "singleton": true,
          "requiredVersion": "1.1.0"
        }
      },
      "shareScope": "@sharedscope"
    }
  }
}