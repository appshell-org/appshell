{
  "extends": "../../tsconfig",
  "compilerOptions": {
    "outDir": "./dist",
    "allowSyntheticDefaultImports": true,
    "declaration": false,
    "declarationMap": false,
    "emitDeclarationOnly": false,
    "isolatedModules": true,
    "strict": true,
    "module": "esnext",
    "lib": ["es2019", "es6", "dom", "webworker"],
    "noEmit": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "sourceMap": false
  },
  "include": ["./src/worker"]
}
