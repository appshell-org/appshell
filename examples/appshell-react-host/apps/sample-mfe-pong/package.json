{
  "name": "sample-mfe-pong",
  "version": "0.0.1",
  "description": "",
  "main": "dist/main.js",
  "scripts": {
    "generate:manifest": "appshell generate manifest --config dist/appshell.config.json",
    "register": "appshell register --manifest dist/appshell.manifest.json --registry $APPSHELL_REGISTRY",
    "watch:config": "nodemon --watch dist/appshell.config.json --exec run-s generate:manifest register",
    "start": "dotenv -- run-p start:appshell start:run",
    "start:appshell": "appshell start --registry $APPSHELL_REGISTRY",
    "start:run": "webpack-cli serve --mode development",
    "build": "webpack --mode production && tsc --emitDeclarationOnly",
    "clean": "rm -rf dist",
    "serve": "serve dist -p $SAMPLE_MFE_PONG_PORT",
    "stats": "webpack --json > stats.json"
  },
  "keywords": [],
  "author": "",
  "license": "MIT"
}